<!-- index.nunjucks -->
{% extends "layout.dev.nunjucks" %}

<!--target is to create pages here instead of app.js and precompile in index-->
{% block content %}
{% include "partials/navigation.nunjucks" %}
{% for point in post_content %}
<!--tooltip-->
<div class="map-popover tooltip top" id="{{ point.title.rendered }}-tooltip">
  <h3 class="map-popover-title">{{ point.acf.card_title }}</h3>
</div>
<!--popup-->
<div class="map-popover popup right popup" id="{{ point.title.rendered }}-popup">
  <div class="arrow"></div>  
  <button class="close">
    <span class="visually-hidden">Close</span>
    <i class="icon close-popup"></i>
  </button>
  <h3 class="map-popover-title">{{ point.acf.card_title }}</h3>
  <p class="map-popover-content">
    {{ point.acf.card_summary }} 
    <button class="open_page">Συνέχισε!</button>
  </p>
</div>
<!--page-->
<div class="overlay overlay" id="{{ point.title.rendered }}-page">
  <div class="page slide page">
    <button class="close">
      <span class="visually-hidden">Close</span>
      <i class="icon close-page-color"></i>
    </button>

    <!--header-->
    <header class="page-header"> 
      <h2 class="outer-point-title">{{ point.acf.card_title }}</h2>
      <div class="progress-bar">
      <!--   <div class="progress-bar-line"></div> -->
        {% for card in point.acf.cards %} 
        <div class="symbol-container symbol
        {% if card.acf_fc_layout === 'intro' %}-intro{% elif card.acf_fc_layout === 'text' %}{% if card.type == 'ΝΕΡΟΔΙΑΒΑΣΜΑΤΑ' %}-study{% elif card.type == 'ΔΡΑΣΤΗΡΙΟΤΗΤΑ ΣΕ ΟΜΑΔΕΣ' %}-group-activity{% elif card.type == 'ΑΤΟΜΙΚΗ ΔΡΑΣΤΗΡΙΟΤΗΤΑ' %}-single-activity{% elif card.type == 'ΕΡΩΤΗΣΗ' %}-question{% elif card.type == 'ΑΠΟ ΕΔΩ ΚΑΙ ΠΕΡΑ' %}-going-forward{% endif %}{% elif card.acf_fc_layout === 'likert' %}-feedback{% else %}-play-learn{% endif %}">
          <i class="icon symbol{% if card.acf_fc_layout === 'intro' %}-intro{% elif card.acf_fc_layout === 'text' %}{% if card.type == 'ΝΕΡΟΔΙΑΒΑΣΜΑΤΑ' %}-study{% elif card.type == 'ΔΡΑΣΤΗΡΙΟΤΗΤΑ ΣΕ ΟΜΑΔΕΣ' %}-group-activity{% elif card.type == 'ΑΤΟΜΙΚΗ ΔΡΑΣΤΗΡΙΟΤΗΤΑ' %}-single-activity{% elif card.type == 'ΕΡΩΤΗΣΗ' %}-question{% elif card.type == 'ΑΠΟ ΕΔΩ ΚΑΙ ΠΕΡΑ' %}-going-forward{% endif %}{% elif card.acf_fc_layout === 'likert' %}-feedback{% else %}-play-learn{% endif %}"></i>
        </div>
        {% endfor %}
      </div>
    </header>
    <!--header-->

    <!--main-->
    <section class="page-content">
      <main class="cards">
      {% for card in point.acf.cards %} 
        {% if card.acf_fc_layout === 'intro' %}
        <div class="card">
          <div class="intro">
            {% if card.block_1 %}
            <div class="col">
              <article class="block_1">
                <h3>ΜΕ ΔΥΟ ΛΟΓΙΑ</h3>
                {{  card.block_1 }}
              </article>
              {% if card.level %}
              <article class="symbols">
                {% if card.duration %}
                <p class="symbol">
                  <i class="icon symbol-activity-duration"></i>
                  <span>{{  card.duration }}</span>
                </p>
                {% endif %}
                {% if card.level %}
                <p class="symbol">
                  <i class="icon symbol-age-group"></i>
                  <span>{{  card.level }}</span>
                </p>
                {% endif %}
                {% if card.classroom === true %}
                <p class="symbol">
                  <i class="icon symbol-indoor"></i>
                  <span>ΕΝΤΟΣ ΤΑΞΗΣ</span></p>
                {% elif card.classroom === false %}
                <p class="symbol">
                  <i class="icon symbol-outdoor"></i>
                  <span>ΕΚΤΟΣ ΤΑΞΗΣ</span></p>
                {% endif %}
              </article>
              {% endif %}
            </div>
            {% endif %}
            <div class="col">
              {% if card.block_2 %}
              <article class="block_2">
                <h3>ΣΤΟΧΟΙ</h3>
                {{  card.block_2 }}
              </article>
              {% endif %}
              {% if card.block_3 %}
              <article class="block_3">
                <h3>ΘΑ ΧΡΕΙΑΣΤΕΙΣ</h3>
                {{  card.block_3 }}
              </article>
              {% endif %}
            </div> 
          </div>
        </div>
        {% elif card.acf_fc_layout === 'text' %}
        <div class="card text">
          {% if card.type == 'ΝΕΡΟΔΙΑΒΑΣΜΑΤΑ' %}
          <h2>ΝΕΡΟΔΙΑΒΑΣΜΑΤΑ</h2>
          {% elif card.type == 'ΔΡΑΣΤΗΡΙΟΤΗΤΑ ΣΕ ΟΜΑΔΕΣ' %}
          <h2>ΔΡΑΣΤΗΡΙΟΤΗΤΑ ΣΕ ΟΜΑΔΕΣ</h2>
          {% elif card.type == 'ΑΤΟΜΙΚΗ ΔΡΑΣΤΗΡΙΟΤΗΤΑ' %}
          <h2>ΑΤΟΜΙΚΗ ΔΡΑΣΤΗΡΙΟΤΗΤΑ</h2>
          {% elif card.type == 'ΕΡΩΤΗΣΗ' %}
          <h2>ΕΡΩΤΗΣΗ</h2>
          {% elif card.type == 'ΑΠΟ ΕΔΩ ΚΑΙ ΠΕΡΑ' %}
          <h2>ΑΠΟ ΕΔΩ ΚΑΙ ΠΕΡΑ</h2>
          {% endif %}
          {{  card.text }}
        </div>
        {% else %}
          {% set quiz_counter  = ( quiz_counter | default(0) ) + 1 %}
          <div class="card quiz quiz-{{ quiz_counter}}">
          {% if card.acf_fc_layout === 'likert' %}<h2>ΠΕΣ ΜΑΣ ΤΗ ΓΝΩΜΗ ΣΟΥ</h2>{% else %}<h2>ΠΑΙΖΩ ΚΑΙ ΜΑΘΑΙΝΩ</h2>{% endif %}
        {% if card.acf_fc_layout === 'dragndrop' %}
        {% set dnd_counter  = ( dnd_counter | default(0) ) + 1 %}
        <section class="text quiz">
          {{ card.intro_text }}
          <div id="{{ point.title.rendered }}-quiz-dnd-{{ dnd_counter }}" class="quiz-dnd">
            <ul class="dnd-target-container">
              {% for target in card.items %}
              {% set target_counter  = ( target_counter | default(-1) ) + 1 %}
              {% set origin_counter  = ( origin_counter | default(0) ) + 1 %}
              <li class="dnd-target dnd-label">
                <span class="text">{{ target.target }}</span>
                <span class="drag-handle-target" data-identifier="{{ point.title.rendered }}-quiz-dnd-{{ dnd_counter }}-{{ target_counter }}"></span>
                <span class="right-positions" data-identifier="{{ point.title.rendered }}-quiz-dnd-{{ dnd_counter }}-{{ target_counter }}">{{ origin_counter }}</span>
              </li>
              {% endfor %}
            </ul>
            <ul class="dnd-origin-container">
              {% for target in card.items %}
              {% set clone_counter  = ( clone_counter | default(0) ) + 1 %}
              {% set origin_counter  = ( origin_counter | default(-1) ) + 1 %}
              <li class="dnd-origin dnd-label">
                <span class="drag-handle" data-identifier="{{ point.title.rendered }}-quiz-dnd-{{ dnd_counter }}-{{ origin_counter }}">{{ clone_counter }}</span>
                <span class="clone">{{ clone_counter }}</span>
                <span class="text">{{ target.option }}</span>
              </li>
              {% endfor %}
            </ul>
          </div>
          <button class="check-dnd submit-button visually-hidden">Ελεγξέ το!</button>
          <button class="reset-dnd submit-button visually-hidden">Δες τις σωστές θέσεις!</button>
          <button class="clear-dnd submit-button visually-hidden">Παίξε ξανά!</button><span class="check-answer"></span>
        </section>
        {% endif %}
        {% if card.acf_fc_layout === 'hotspot' %}
        {% set hotspot_counter  = ( hotspot_counter | default(0) ) + 1 %}
        <section class="text">
          {{ card.hotspot_description }}
          <div id="{{ point.title.rendered }}-quiz-hotspot-{{ quiz_counter }}" class="quiz-hotspot {{ card.hotspot_type }}">
            <figure class="hotspot-image" style="width:{{ card.hotspot_file.width }}px;height:{{ card.hotspot_file.height }}px;">
              <img src="{{ card.hotspot_file.url }}" width="{{ card.hotspot_file.width }}" height="{{ card.hotspot_file.height }}">
            </figure>
            {% if card.hotspot_type === 'drag-and-drop' %}
            <ul class="hotspot-label-list">
              {% for hotspot in card.hotspots %}
              {% set hotspot_label_counter_inner  = ( hotspot_label_counter_inner | default(-1) ) + 1 %}
              {% set hotspot_label_counter_outer  = ( hotspot_label_counter_outer | default(0) ) + 1 %}
              <li>
              <div class="hotspot-labels">
                <div class="draggable-item hotspot-handle" data-identifier="{{ point.title.rendered }}-quiz-hotspot-{{ quiz_counter }}-point--{{ hotspot_label_counter_inner }}">{{ hotspot_label_counter_outer }}</div>
                <p class="text">{{ hotspot.hotspot_title }} </p>
              </div>
              </li>
              {% endfor %}
            </ul>
            {% endif %}
          </div>
          <button class="check-hotspot submit-button visually-hidden">Ελεγξέ το!</button>
          <button class="reset-hotspot submit-button visually-hidden">Δες τις σωστές θέσεις!</button>
          <button class="clear-hotspot submit-button visually-hidden">Παίξε ξανά!</button><span class="check-answer"></span>
        </section>
      {% elif card.acf_fc_layout === 'sortable' %}
      <section class="text quiz-sortable">
        {% if card.sortable_text %} 
          {{  card.sortable_text }}
        {% endif %}
        <div class="sortable-container">
          {% for option in card.sortable_options %}
          {% set options_counter  = ( options_counter | default(0) ) + 1 %}
          <div class="list-item list-item-{{ options_counter }}">
            <div class="item-content">
              <span class="right-order">{{ option.right_order }}</span>
              <span class="user-input-order order"></span> 
              <span class="label">{{  option.quiz_input_value }}</span>
            </div>
          </div>
          {% endfor %}
        </div>
      </section>
      {% elif card.acf_fc_layout === 'multiple_choice_multiple' %}
        {{ card.quiz_description }}
        {% for option in card.quiz %}
        {% set options_counter  = ( options_counter | default(0) ) + 1 %}
        <form id="gform-{{ point.title.rendered }}-{{ quiz_counter }}-{{ options_counter }}" class="multiple-choice" action="{{  option.quiz_form_url }}" method="POST">
          <fieldset>
            <legend 
              for="{{  option.quiz_field_name }}" 
              name="{{ option.quiz_field_name }}">
              {{  option.quiz_title }}{{ option.quiz_extended_text }}
            </legend>
            {% for sub_option in option.quiz_options %}
            {% set sub_options_counter  = ( sub_options_counter | default(0) ) + 1 %}
            <p class="radio">
              <input 
                id="{{  option.quiz_field_name }}-{{ point.title.rendered }}-{{ sub_options_counter }}" 
                name="{{  option.quiz_field_name }}" 
                type="radio" 
                value="{{  sub_option.quiz_input_value }}"
                data-type="{% if sub_option.quiz_input_type === 'Σωστό' %}correct{% else %}wrong{% endif %}" 
              />
              <label for="{{  option.quiz_field_name }}-{{ point.title.rendered }}-{{ sub_options_counter }}">
                <span></span>
                {{  sub_option.quiz_input_value }}
              </label>
            </p>
            {% endfor %}
          </fieldset>
      </form>
      <button id="gform-{{ point.title.rendered }}-{{ quiz_counter }}-{{ options_counter }}--submit" class="button submit submit-button">
        Δες τη σωστή απάντηση!
        <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
      </button>
      <small id="gform-{{ point.title.rendered }}-{{ quiz_counter }}-{{ options_counter }}--form-error" class="form-error"></small>
      <article id="gform-{{ point.title.rendered }}-{{ quiz_counter }}-{{ options_counter }}--thankyou_message" class="thankyou_message" style="display: none;">
        <p><span class="check-answer"></span><br> {{  option.quiz_thank_you_message }}</p>
        <small>Ευχαριστούμε για την απάντηση σου!</small>
        <button id="gform-{{ point.title.rendered }}-{{ quiz_counter }}-{{ options_counter }}--reset" class="button reset submit-button visually-hidden">
          Προσπάθησε ξανά!
          <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
        </button>
      </article>
      {% endfor %}
      {% elif card.acf_fc_layout === 'short_text' %}
      <form id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}" class="short-text feedback" action="{{  card.form_url }}" method="POST">
        <fieldset>
          <legend>{{  card.form_title }}</legend>
          <textarea
            id="{{  card.form_field_name }}-{{ point.title.rendered }}" 
            name="{{  card.form_field_name }}"
            rows="10" cols="50" wrap="physical" placeholder="Γράψε κάτι εδώ!"
          ></textarea>
        </fieldset>
      </form>
      <button id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}--submit" class="button submit submit-button">
        Υποβολή!
        <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
      </button>
      <small id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}--form-error" class="form-error"></small>
      <article id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}--thankyou_message" class="thankyou_message" style="display: none;">
        <small>Ευχαριστούμε για την απάντηση σου!</small>
      </article>
      {% elif card.acf_fc_layout === 'feedback' %}
        {% for type in card.type %} 
        {% if type.acf_fc_layout === 'short_text' %}
        <form id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}" class="short-text feedback" action="{{  type.form_url }}" method="POST">
          <fieldset>
            <legend>{{  type.form_description }}</legend>
            <textarea
              id="{{  type.form_field_name }}-{{ point.title.rendered }}" 
              name="{{  type.form_field_name }}"
              rows="10" cols="50" wrap="physical" placeholder="Γράψε κάτι εδώ!"
            ></textarea>
          </fieldset>
        </form>
        <button id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}--submit" class="button submit submit-button">
          Υποβολή!
          <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
        </button>
        <small id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}--form-error" class="form-error"></small>
        <article id="gform-{{ point.title.rendered }}-short_text-{{ quiz_counter }}--thankyou_message" class="thankyou_message" style="display: none;">
          <small>Ευχαριστούμε για την απάντηση σου!</small>
        </article>
        {% endif %}
        {% endfor %}
      {% elif card.acf_fc_layout === 'multiple_choice' or card.acf_fc_layout === 'checkbox' or card.acf_fc_layout === 'likert' %}
      <form id="gform-{{ point.title.rendered }}-{{ quiz_counter }}" class="{% if card.acf_fc_layout === 'multiple_choice' %}multiple-choice{% elif card.acf_fc_layout === 'checkbox' %}checkbox-quiz{% elif card.acf_fc_layout === 'likert' %}likert{% endif %}" action="{{  card.quiz_form_url }}" method="POST">
        <fieldset>
          <legend for="{{  card.quiz_field_name }}{{ options_counter }}" 
          name="{% if card.acf_fc_layout === 'multiple_choice' or card.acf_fc_layout === 'likert' %}{{ card.quiz_field_name }}{% elif card.acf_fc_layout === 'checkbox' %}{{  card.quiz_field_name }}{{ options_counter }}{% endif %}">{{  card.quiz_title }}{{ card.quiz_extended_text }}</legend>
          {% if card.acf_fc_layout === 'multiple_choice' or card.acf_fc_layout === 'checkbox'  %}
          {% for option in card.quiz_options %}
          {% set options_counter  = ( options_counter | default(0) ) + 1 %}
          <p class="{% if card.acf_fc_layout === 'multiple_choice' %}radio{% elif card.acf_fc_layout === 'checkbox' %}checkbox{% elif card.acf_fc_layout === 'likert' %}likert{% endif %}">
            <input 
            id="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}" 
            name="{% if card.acf_fc_layout === 'multiple_choice' %}{{  card.quiz_field_name }}{% elif card.acf_fc_layout === 'checkbox' %}{{  option.quiz_input_value  }}{% endif %}" 
            type="{% if card.acf_fc_layout === 'multiple_choice' %}radio{% elif card.acf_fc_layout === 'likert' %}radio{% elif card.acf_fc_layout === 'checkbox' %}checkbox{% endif %}" 
            value="{{  option.quiz_input_value }}"
            data-type="{% if option.quiz_input_type === 'Σωστό' %}correct{% else %}wrong{% endif %}" 
            />
            <label for="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}">
            <span></span>
            {{  option.quiz_input_value }}
            </label>
            {% if card.acf_fc_layout === 'checkbox' %}
            {% if option.photo %}
            <br><img src="{{ option.photo.url }}" alt="{{ option.quiz_input_value }}" class="checkbox-photo">
            {% endif %}
            {% if option.quiz_prompt %}
            <span class="checkbox-prompt">{{ option.quiz_prompt }}</span>
            {% endif %}
            {% endif %}
          </p>
          {% endfor %}
          {% endif %}
          {% if card.acf_fc_layout === 'likert' %}
          {% set options_counter  = ( options_counter | default(0) ) + 1 %}
          <div class="likert-container">
            <p class="likert">
              <input 
              id="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}"
              type="radio"
              name="{{  card.quiz_field_name }}"
              value="καθόλου"
              >
              <label for="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}">
                <span></span>
                <mark>καθόλου</mark>
            </p>
            <p class="likert">
              <input 
                id="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}"
                type="radio"
                name="{{  card.quiz_field_name }}"
                value="λίγο"
              />
              <label for="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}">
                <span></span>
                <mark>λίγο</mark>
              </label>
            </p>
          <p class="likert">
          <input 
          id="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}"
          type="radio"
          name="{{  card.quiz_field_name }}"
          value="μέτρια"
          />
          <label for="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}">
          <span></span>
          <mark>μέτρια</mark>
          </label>
          </p>
          <p class="likert">
          <input 
          id="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}"
          type="radio"
          name="{{  card.quiz_field_name }}"
          value="αρκετά"
          />
          <label for="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}">
          <span></span>
          <mark>αρκετά</mark>
          </label>
          </p>
          <p class="likert">
          <input 
          id="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}"
          type="radio"
          name="{{  card.quiz_field_name }}"
          value="πάρα πολύ"
          />
          <label for="{{  card.quiz_field_name }}-{{ point.title.rendered }}-{{ options_counter }}">
          <span></span>
          <mark>πάρα πολύ</mark>
          </label>
          </p>
          </label>
          {% endif %}
        </fieldset>
      </form>

      {% endif %}
      {% if card.acf_fc_layout === 'multiple_choice' %}
      <button id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--submit" class="button submit submit-button">
        Δες τη σωστή απάντηση!
        <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
      </button>
      {% endif %}
      {% if card.acf_fc_layout === 'checkbox' %}
      <button id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--submit" class="button submit submit-button">
        Δες τις σωστές απαντήσεις!
        <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
      </button>
      {% endif %}
      {% if card.acf_fc_layout === 'sortable' %}
      <button id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--resort" class="button resort submit-button">Δες τη σωστή σειρά!
        <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
      </button>
      {% endif %}

      {% if card.acf_fc_layout === 'likert' %}
      <div class="loader-container">
        <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
      </div>
      {% endif %}
      {% if card.acf_fc_layout === 'checkbox' or card.acf_fc_layout === 'multiple_choice' %}
      <small id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--form-error" class="form-error"></small>
      <article id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--thankyou_message" class="thankyou_message" style="display: none;">
        <p><span class="check-answer"></span><br> {{  card.quiz_thank_you_msg }}</p>
        <small>Ευχαριστούμε για την απάντηση σου!</small>
        <button id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--reset" class="button reset submit-button visually-hidden">
          Προσπάθησε ξανά!
          <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="loader quiz" viewBox="0 0 100 100"><use xlink:href="#submit-loader"></use></svg>
        </button>
      </article>
      {% elif card.acf_fc_layout === 'likert' %}
      <article id="gform-{{ point.title.rendered }}-{{ quiz_counter }}--" class="thankyou_message" style="display: none;">
      <small>Ευχαριστούμε για την απάντηση σου!</small>
      </article>
      {% endif %} 
      </div>

      {% endif %}
      {% endfor %}

   
      </main>
    </section>
